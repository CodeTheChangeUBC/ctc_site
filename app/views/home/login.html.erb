<% provide(:title, 'login') %>

    <div class= "row center login_page">
    <h1>CTC Login</h1>
    <p>Future Login Page<p>
        </div>
<div class="row center">
    <%= form_for :home do |f| %>
        <p>
        <%= f.label :user %><br>
        <%= f.text_field :user %><br>
        </p>
        <p>
        <%= f.label :password %><br>
        <%= f.text_field :password %><br>
        </p>
        <p>
        <%= f.submit %>
        </p>
    <% end %>

    <p>Not a member yet?</p>
    <% button_to "Sign Up!", new_member_path, :method => "get", class: "btn btn-primary btn-lg" %> 
</div>
